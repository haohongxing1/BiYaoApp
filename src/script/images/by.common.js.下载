var byScope={statistical:function(){var e,t,o="?";if(window.performance&&window.performance.timing&&void 0!=window.performance.timing)if(window.performance.timing.loadEventEnd){e=window.performance.timing,t={startOpen:e.navigationStart,requestStart:e.requestStart,responseStart:e.responseStart,responseEnd:e.responseEnd,domLoading:e.domLoading,domInteractive:e.domInteractive,domComplete:e.domComplete};for(var n in t)o+=n+"="+t[n]+"&";o=o.slice(0,o.length-1),$.ajax({dataType:"jsonp",type:"post",jsonp:"jsonpcallback",data:"",url:window.ControllerSite+"/common/statistical"+o,contentType:"application/x-www-form-urlencoded;charset=utf-8",success:function(){console.log("Page record successÔºÅ")}})}else setTimeout(function(){byScope.statistical()},500)}};